{
  "name": "ruark/laravel-inn",
  "description": "Laravel package for INN Validation (Taxpayer identification number in Russia)",
  "keywords": [
    "inn",
    "validation",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yuriy Lysyanskii",
      "email": "yl.ruark@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "illuminate/support": ">=7.4.0",
    "illuminate/validation": ">=5.8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^10.5|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Ruark\\LaravelInn\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruark\\LaravelInn\\Tests\\": "tests/"
    }
  }
}
